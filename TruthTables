.data
heading1:.asciiz"Truth Table of AND gate:"
heading2:.asciiz"Truth Table of OR gate:"
heading3:.asciiz"Truth Table of XOR gate:"
heading4:.asciiz"Truth Table of NOR gate:"
heading5:.asciiz"Truth Table of XNOR gate:"
heading6:.asciiz"Truth Table of NAND gate:"
newline:.asciiz"\n"
.text
.globl main
main:
li $t0,0x00000000
li $t1,0x00000001
li $v0,4
la $a0,heading1
syscall
li $v0,4
la $a0,newline
syscall
and  $t3,$t0,$t0
and $t4,$t0,$t1
