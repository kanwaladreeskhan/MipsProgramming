.data
input:.asciiz"Enter number to calculate factorial:"
 .text
.globl main
main:
 li $v0, 4
   la $a0, input
   syscall
   li $v0, 5
   syscall
   move $t1, $v0
   li $t0,1 #mul
while:
bnez $t1,factorial
j exit
factorial:
mul $t0,$t0,$t1
 sub $t1,$t1,1
j while
exit:
li $v0,1
move $a0,$t0
syscall
li $v0,10
syscall
