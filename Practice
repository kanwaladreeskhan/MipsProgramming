.data
msg:.asciiz"Enter 1 to ON a machine and 0 to OFF a machine:"
input:.asciiz"\nEnter the number of machine: "
actualbit: 0x00000000
.text
.globl main
main
li $v0,4
la $a0,msg
syscall
li $v0,5
syscall
move $t0,$v0

li $v0,4
la $a0,input
syscall
li $v0,5
syscall
move $t1,$v0
beq $t1,0,mach1
beq $t1,1,mach2
beq $t1,2,mach3
beq $t1,3,mach4

mach1:

