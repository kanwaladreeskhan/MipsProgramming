.data
input: .asciiz"Enter a number: "
summsg:.asciiz"The sum is:"
.text
.globl main
main:
li $v0,4
la $a0,input
syscall
li $v0,5
syscall
move $t5,$v0
li $t0,2#counter
li $t1,0 #sum
do_while:
add $t1,$t1,$t0 #sum update sum=sum+i
addi $t0,$t0,2  #i=i+2
bgt  $t0,$t5,end_do_while
j do_while
 end_do_while:
 li $v0,1
move $a0,$t1
syscall
li $v0,10
syscall
