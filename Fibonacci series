.data
num1Msg:.asciiz "Please enter number  : "
resultMsg:.asciiz "The Fibonacci series is : "
comma:.asciiz","
.text
.globl main
main:
li $v0,4
la $a0,num1Msg
syscall
li $v0,5  
syscall
move $t6,$v0
li $t0,0
li $t1,1
add $t3,$zero,1
li $v0,4
la $a0,resultMsg
syscall
li $v0,1
move $a0 ,$t0
syscall
loop:
li $v0,4
la $a0,comma
syscall
li $v0,1
move $a0 ,$t1
syscall
add $t3,$t3,1
add $t2,$t1,$t0
move $t0, $t1
move $t1, $t2
sub $t5,$t6,$t3
beqz $t5, exit
j loop
 exit:
 li $v0,10
 syscall

