.data
num1:.word 30
num2:.word 40
result:.word 0
resultMsg:.asciiz"The result is : "
newline:.asciiz"\n"
.text
.globl main
main:
li $t0,30
li $t1, 40
addu $t2,$t1,$t0
li $v0,4
la $a0,resultMsg
syscall
move $a0,$t2
li $v0,1
syscall 
li $v0,4
la $a0, newline
syscall 
lw $t0,num1 # 2nd method
lw $t1, num2
addu,$t2,$t1,$t0
sw $t2,result   #save in result variable 
li $v0,4
la $a0,resultMsg
syscall
lw $a0,result
li $v0,1
syscall
li $v0,10
syscall